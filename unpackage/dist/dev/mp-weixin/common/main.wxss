	switch,
	textarea,
	navigator,
	audio,
	camera,
	image,
	video {
		box-sizing: border-box;
}

/* 骨架屏替代方案 */
.Skeleton {
		background: #f3f3f3;
		padding: 20rpx 0;
		border-radius: 8rpx;
}

/* 图片载入替代方案 */
.image-wrapper {
		font-size: 0;
		background: #f3f3f3;
		border-radius: 4px;
image {
			width: 100%;
			height: 100%;
			-webkit-transition: .6s;
			transition: .6s;
			opacity: 0;
&.loaded {
				opacity: 1;
}
}
}
.clamp {
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
		display: block;
}
.common-hover {
		background: #f5f5f5;
}

/*边框*/
.b-b:after,
	.b-t:after {
		position: absolute;
		z-index: 3;
		left: 0;
		right: 0;
		height: 0;
		content: '';
		-webkit-transform: scaleY(.5);
		        transform: scaleY(.5);
		border-bottom: 1px solid $border-color-base;
}
.b-b:after {
		bottom: 0;
}
.b-t:after {
		top: 0;
}

/* 新加 */
.b-bs:after{
		position: absolute;
		z-index: 3;
		left: 0;
		top: auto;
		bottom: 0;
		right: 0;
		height: 0;
		content: '';
		-webkit-transform: scaleY(.5);
		        transform: scaleY(.5);
		border-bottom: 1px solid #e0e0e0;
}
.b-ts:before{
		position: absolute;
		z-index: 3;
		left: 0;
		top: 0;
		right: 0;
		height: 0;
		content: '';
		-webkit-transform: scaleY(.5);
		        transform: scaleY(.5);
		border-bottom: 1px solid #e5e5e5;
}
.b-rs:after{
		position: absolute;
		z-index: 3;
		right: 0;
		top: 0;
		bottom: 0;
		width: 0;
		content: '';
		-webkit-transform: scaleX(.5);
		        transform: scaleX(.5);
		border-right: 1px solid #e5e5e5;
}
.b-ls:before{
		position: absolute;
		z-index: 3;
		left: 0;
		top: 0;
		bottom: 0;
		width: 0;
		content: '';
		-webkit-transform: scaleX(.5);
		        transform: scaleX(.5);
		border-left: 1px solid #e5e5e5;
}
.b-bs, .b-ts, .b-ls, .b-rs{
		position: relative;
}

/* button样式改写 */
uni-button,
	button {
		height: 80rpx;
		line-height: 80rpx;
		font-size: $font-lg + 2rpx;
		font-weight: normal;
&.no-border:before,
		&.no-border:after {
			border: 0;
}
}
uni-button[type=default],
	button[type=default] {
		color: $font-color-dark;
}

